{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://veupathdb.github.io/docs-api-schema/schemata/errors/401.json",
  "title": "Unauthorized",
  "description": "401 error response body.",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "unauthorized"
      ]
    },
    "message": {
      "type": "string"
    }
  },
  "required": [
    "status",
    "message"
  ],
  "additionalProperties": false,
  "examples": [
    {
      "status": "unauthorized",
      "message": "users must be logged in to access this resource"
    }
  ]
}
