{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://veupathdb.github.io/docs-api-schema/schemata/errors/404.json",
  "title": "Not Found",
  "description": "404 error response body.",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "not-found"
      ]
    },
    "message": {
      "type": "string"
    }
  },
  "required": [
    "status",
    "message"
  ],
  "additionalProperties": false,
  "examples": [
    {
      "status": "not-found",
      "message": "the requested resource could not be found"
    }
  ]
}
