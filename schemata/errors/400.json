{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://veupathdb.github.io/docs-api-schema/schemata/errors/400.json",
  "title": "Bad Request",
  "description": "400 error response body.",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "bad-request"
      ]
    },
    "message": {
      "type": "string"
    }
  },
  "required": [
    "status",
    "message"
  ],
  "additionalProperties": false,
  "examples": [
    {
      "status": "bad-request",
      "message": "malformed JSON"
    }
  ]
}
